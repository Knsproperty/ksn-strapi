    "referenceNumber": {
      "type": "string",
      "required": true
    },
    "agentBRN": {
      "type": "integer",
      "required": true
    },
    "agent": {
      "type": "string",
      "required": true
    },
    "bedrooms": {
      "type": "integer",
      "required": true
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "area": {
      "type": "integer",
      "required": true
    }
